<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
         <record id="certificate_serial" model="ir.sequence.type">
            <field name="name">CS</field>
            <field name="code">certificate.serial</field>
         </record>
        <record id="certificate_serial_" model="ir.sequence">
            <field name="name">Certificate Serial</field>
            <field name="code">certificate.serial</field>
            <field name="padding">3</field>
        </record>

         <record id="sara_inv_serial" model="ir.sequence.type">
            <field name="name">SIS</field>
            <field name="code">sara.inv.serial</field>
         </record>
        <record id="sara_inv_serial_" model="ir.sequence">
            <field name="name">Sara Invoice Serial</field>
            <field name="code">sara.inv.serial</field>
            <field name="padding">4</field>
            <field name="suffix">/%(month)s/%(year)s</field>
        </record>

        <record id="abc_inv_serial" model="ir.sequence.type">
            <field name="name">AIS</field>
            <field name="code">abc.inv.serial</field>
         </record>
        <record id="abc_inv_serial_" model="ir.sequence">
            <field name="name">ABC Invoice Serial</field>
            <field name="code">abc.inv.serial</field>
            <field name="padding">4</field>
            <field name="suffix">/%(month)s/%(year)s</field>
        </record>
    </data>
</openerp>