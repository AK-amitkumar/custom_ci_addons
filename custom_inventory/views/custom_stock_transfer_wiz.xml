<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
         <record model="ir.ui.view" id="custom_stock_tranfer_wiz_form">
             <field name="name">Enter transfer details</field>
             <field name="model">stock.transfer_details</field>
             <field name="inherit_id" ref="stock.view_stock_enter_transfer_details" />
             <field name="context">{'location':False}</field>
             <field name="arch" type="xml">
                 <xpath expr="//field[@name='item_ids']" position="attributes">
                     <attribute name="context">{'location':False}</attribute>
                 </xpath>
                 <xpath expr="//tree/field[@name='destinationloc_id']" position="after">
                     <field name="engine_number" />
                     <field name="chassis_number" />
                     <field name="color" />
                     <field name="model"/>
                     <field name="year"/>
                 </xpath>
                 <!--invisible="context.get('location')==False"-->
             </field>
         </record>
    </data>
</openerp>